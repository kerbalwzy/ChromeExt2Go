{
  "manifest_version": 2,
  "name": "ChromeExt2Go",
  "description": "https://github.com/kerbalwzy/ChromeExt2Go",
  "version": "0.0.1",
  "icons": {
    "16": "assets/logo.png",
    "48": "assets/logo.png",
    "128": "assets/logo.png"
  },
  "browser_action": {
    "default_popup": "html/popup.html",
    "default_icon": "assets/logo.png"
  },
  "content_security_policy": "script-src 'self' 'unsafe-eval'; object-src 'self'",
  "background": {
    "scripts": [
      "js/hot-reload.js",
      "js/socket.io.1.2.0.js",
      "js/background.js"
    ]
  },
  "web_accessible_resources": [
    "fonts/*",
    "assets/*"
  ]
}